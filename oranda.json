{
  "build": {
    "path_prefix": "twt",
    "dist_dir": "./docs"
  },
  "components": {
    "artifacts": {
      "cargo_dist": true,
      "package_managers": {
        "crates.io": "cargo install twt --locked --profile=dist",
        "binstall": "cargo binstall twt",
        "nix flake": "nix profile install github:msfjarvis/twt"
      }
    }
  }
}
